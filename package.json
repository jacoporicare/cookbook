{
  "name": "cookbook",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "start": "node index",
    "dev": "run-p dev:server dev:webpack",
    "dev:server": "cross-env NODE_ENV=development PORT=4001 nodemon src/server --watch src/server --exec babel-node",
    "dev:webpack": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress --colors --config webpack.config.dev.js --port 4000",
    "build": "run-s clean build:server build:copy build:webpack",
    "build:server": "babel src/server -d dist",
    "build:copy": "shx cp package.json Dockerfile dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.prod.js",
    "clean": "shx rm -rf dist",
    "test": "jest",
    "test:watch": "yarn test -- --watch"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-plugin-react-css-modules": "^2.7.1",
    "babel-polyfill": "^6.16.0",
    "body-parser": "^1.17.2",
    "bootstrap-sass": "^3.3.7",
    "composable-middleware": "^0.3.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.3",
    "express-jwt": "^5.1.0",
    "font-awesome": "^4.7.0",
    "javascript-remove-diacritics": "^1.0.0",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.10.3",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-autosuggest": "^9.0.1",
    "react-bootstrap": "^0.31.0",
    "react-cookie": "^2.0.7",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-router-scroll": "^0.4.1",
    "react-sortable-hoc": "^0.6.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "slug": "^0.9.1",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.3",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.1.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.24.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.5",
    "react-test-renderer": "^15.4.2",
    "sass-loader": "^6.0.5",
    "shx": "^0.2.2",
    "style-loader": "^0.18.1",
    "unexpected": "^10.29.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js"
    }
  }
}
