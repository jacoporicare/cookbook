{
  "name": "cookbook",
  "private": true,
  "workspaces": [
    "api",
    "web"
  ],
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "dev": "concurrently \"yarn workspace cookbook-api start\" \"yarn workspace cookbook-web dev\"",
    "format": "yarn workspaces foreach -Apt run format",
    "format:check": "yarn workspaces foreach -Apt run format:check"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
