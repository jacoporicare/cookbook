{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "class-methods-use-this": 0,
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": ["**/*.test.js", "**/*.spec.js", "webpack.config.*.js"]
    }],
    "max-len": 0,
    "no-plusplus": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0
  }
}
