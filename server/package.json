{
  "name": "cookbook-server",
  "version": "0.2.2",
  "private": true,
  "scripts": {
    "start": "node index",
    "dev": "PORT=4001 nodemon src --watch src --exec babel-node",
    "prebuild": "rm -rf dist",
    "build": "babel src -d dist && cp package.json yarn.lock dist",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "prettier": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "composable-middleware": "^0.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "express-jwt": "^5.1.0",
    "jsonwebtoken": "^7.4.1",
    "mongoose": "^4.10.4",
    "slug": "^0.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-preset-env": "^1.5.1",
    "babel-preset-stage-1": "^6.16.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-prettier": "^2.1.1",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "prettier": "^1.5.2",
    "unexpected": "^10.29.0"
  }
}
